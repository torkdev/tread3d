// Defines global info for Daikatana
$DefGlobal [ "Daikatana" ]					// Basic game defaults
{
	def_author		: "Wayne Smith"               // Author
	def_version		: "2.0"                       // Version
	def_solid  		: "func_door"			// Solid entity
	def_point  		: "info_player_deathmatch"	// Point entity
	def_gamemaptype   : "QUAKE2_MAP"			// Type of MAP file to generate
	def_textype		: "@DAIWAL"                   // Texture Format
	def_pakdir		: "data"                      // Location of .pak file
	def_targetkey	: "target"
	def_namekey		: "targetname"
	def_worldspawn	: "worldspawn"

	$DefBuild
	{
		$DefModule [ @@MapDir : "Map Directory" : "" ] 
		{
			def_pathtype : @DIR
			def_type : @NOT_RUNNABLE
			def_path : "[ENTER PATH HERE]"
		}

		$DefModule [ @@GameDir : "Daikatana Directory" : "" ] 
		{
			def_pathtype : @DIR
			def_type : @NOT_RUNNABLE
			def_path : "[ENTER PATH HERE]"
		}

		$DefModule [ @@MapFile : "" : "" ]
		{
			def_type : @RUNNABLE

			$DefParams
			{
				def_param : "" : @@MapDir : @ALWAYS : false
				def_param : "" : "\\" : @ALWAYS : false
				def_param : "" : @FILENAME : @ALWAYS : false
				def_param : "" : ".map" : @ALWAYS : false
			}
			
			$ExportMap : @@MapFile : "X-porting map..."
		}

		$DefModule [ @@BspSection : "BSP Program" : "BSP Options" ]
		{
			def_pathtype : @FILE
			def_type : @RUNNABLE
			def_path : "[ENTER PATH HERE]"

			$DefParams
			{
				def_param : "None" 		: @ABORT 		: 1 		: false
				def_param : "Only Ents" 	: "-onlyents " 	: 2 		: false
				def_param : "Verbose" 		: "-v " 		: 4 		: false
				def_param : "No T Junctions" 	: "-notjunc " 	: 8 		: false
				def_param : "No Fill" 		: "-nofill " 	: 16 		: false
				def_param : "No CSG" 		: "-nocsg " 	: 32 		: false
				def_param : "No Detail" 	: "-nodetail " 	: 64 		: false
				def_param : "No Merge" 		: "-nomerge " 	: 128 	: false
				def_param : "No Share" 		: "-noshare " 	: 256 	: false
				def_param : "No Subdivision" 	: "-nosubdiv " 	: 512 	: false
				def_param : "No Optimize" 	: "-noopt " 	: 1024 	: false
				def_param : "No Prune" 		: "-noprune " 	: 2048 	: false
				def_param : "No Water" 		: "-nowater " 	: 4096 	: false
				def_param : "No Weld" 		: "-noweld " 	: 8192 	: false
				def_param : "Full Detail" 	: "-fulldetail " 	: 16384 	: false
				def_param : "Leak Test" 	: "-leaktest " 	: 32768 	: false
				 				
				def_param : "" : @@MapFile : @ALWAYS : false 
			}
			
			$SpawnWait : @THIS : @@MapDir : @CONSOLE : "Execing Daikatana BSP"
		}

		$DefModule [ @@BspFile : "" : "" ]
		{
			def_type : @RUNNABLE

			$DefParams
			{
				def_param : "" : @FILENAME : @ALWAYS : false
			}
		}

		$DefModule [ @@VisSection : "VIS Program" : "VIS Options" ]
		{
			def_pathtype : @FILE
			def_type : @RUNNABLE
			def_path : "[ENTER PATH HERE]"

			$DefParams
			{
				def_param : "None" : @ABORT : 1 : false
				def_param : "Fast" : "-fast " : 2 : false
				def_param : "No Sort" : "-nosort " : 4 : false
				def_param : "Cull Error" : "-cullerror " : 8 : false
				def_param : "Verbose" : "-v " : 16 : false
				def_param : "" : @@BspFile : @ALWAYS : false
			}		

			$SpawnWait : @THIS : @@MapDir : @CONSOLE : "Execing Daikatana VIS"
		}

		$DefModule [ @@RadSection : "RAD Program" : "RAD Options" ]
		{
			def_pathtype : @FILE
			def_type : @RUNNABLE
			def_path : "[ENTER PATH HERE]"

			$DefParams
			{
				def_param : "None" : @ABORT : 1 : false
				def_param : "Extra" : "-extra " : 2 : false
				def_param : "No Block" : "-noblock " : 4 : false
				def_param : "No Color" : "-nocolor " : 8 : false
				def_param : "No Pvs" : "-nopvs " : 16 : false	
				def_param : "" : @@BspFile : @ALWAYS : false
			}

			$SpawnWait : @THIS : @@MapDir : @CONSOLE : "Execing Daikatana RAD"
		}

		$DefModule [ @@Daikatana : "Daikatana Exe" : "Daikatana Options" ] 
		{
			def_pathtype : @FILE
			def_type : @RUNNABLE
			def_path : "[ENTER PATH HERE]"

			$DefParams
			{
				def_param : "None" : @ABORT : 1 : false
				def_param : "" : " +map " : @ALWAYS : false
				def_param : "" : @@BspFile : @ALWAYS : false 
			}

			$SpawnWait : @THIS : @@GameDir : @CONSOLE : "Running Daikatana"
		}
	}
